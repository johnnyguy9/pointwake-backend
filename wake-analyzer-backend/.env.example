# Application Settings
APP_NAME=Wake Analyzer
DEBUG=false

# Security (CHANGE IN PRODUCTION!)
SECRET_KEY=your-super-secret-key-change-this-in-production

# Database
DATABASE_URL=sqlite:///./wake_analyzer.db

